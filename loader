-- Scripts:

local function IXPVK_fake_script() -- Close.Script 
	local script = Instance.new('Script', Close)

	ocal SGui = script.Parent
	local Frame = SGui:WaitForChild("Frame")
	local Button = SGui:WaitForChild("TextButton")
	local Lable = SGui:WaitForChild("TextLable")
	
	Button.MouseButton1Up:Connect(function()
		Frame.Visible = not Frame.Visible
	end)
end
coroutine.wrap(IXPVK_fake_script)()
